<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userQualifi">

	<resultMap type="com.topia.card.User.UserVO" id="qualifi">
		<result column="QUALIFI_IDX" property="qualifiIdx"/>
		<result column="USER_IDX" property="userIdx"/>
		<result column="QUALIFI_NAME" property="qualifiName"/>
		<result column="QUALIFI_GETDATE" property="qualifiGetdate"/>
	</resultMap>
	
	<insert id="insert">
		<foreach collection="qualifiList" item="item" index="index" open="INSERT ALL" separator=" " close="SELECT * FROM DUAL">
		INTO USER_INFO_QUALIFI
			(
			  QUALIFI_IDX
			, USER_IDX
			, QUALIFI_NAME
			, QUALIFI_GETDATE
			)
		VALUES
			(
		 	  USER_INFO_QUALIFI_SEQ.NEXTVAL+#{index}
			, USER_INFO_SEQ.CURRVAL
			, #{item.qualifiName}
			, #{item.qualifiGetdate}
			)
		</foreach>
	</insert>


</mapper>